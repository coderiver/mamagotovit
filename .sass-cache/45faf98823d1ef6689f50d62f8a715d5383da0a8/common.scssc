:@template"%@import "lib/base";


*{margin:0;padding:0;-webkit-text-size-adjust: none; }
html{height: 100%;}
body{
	height: 100%;
	min-width:1400px;
	font: {
		family: OpenSans-l,sans-serif;
		weight: normal;
		size: 12px;
	}
	line-height:1.4;
	background-image: url(../img/fon.png);
}
// resets
a img,fieldset{
	border:none;
	}
a{
	text-decoration: none;
	color:#00919e;
	&:hover{

	}
}
.clr{clear:both;}
// common styles, these could be attached to any block with "@extend %wyswyg;"
%wyswyg{
	p{
		margin-bottom: 1em;
	}
	ul,ol{
		margin: 1em 2em;
		list-style: disc;
	}
	ol{
		list-style: decimal;
	}
	blockquote{
		margin: 1em 2em;
	}
	table{
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		td{
			border: 1px solid #ccc;
			padding: 5px;
		}
	}
}
// css-styles for content-layer
.l-content{
	min-height: 100%;
	height: auto!important;
	height: 100%;
	margin: 0px auto -167px;
	.hFooter{
		height: 167px;
		clear:both;
	}
}
.content{
	width: 1400px;
	margin: 0px auto;
}

// css-styles for header
.header{
	position: relative;
	width: 1400px;
	margin: 0px auto 104px;
}
.tema{
	position: absolute;
	@include sized-sprite($icons, tema);
	z-index: 2;
}
.logo{
	position: relative;
	float: left;
	margin: 5px 33px 0px 195px;
	@include sized-sprite($icons, logo);
	span{
		position: absolute;
		left: 93px;
		font-size: 13px;
		color: #fff;
	}
	width: 161px;
	z-index: 2;
}
.menu{
	position: relative;
	height: 94px;
	padding-top:4px;
	background-image: url(../img/fon-header.png);
}
.menu-item{
	display: inline-block;
	margin-left: 13px;
	vertical-align: top;
}	
.menu__link{
	position: relative;
	display: block;
	max-width: 66px;
	height: 49px;
	padding: 45px 10px 0px;
	@include border-radius(43px 43px 0px 0px);
	text-align: center;
	font-size:12px;
	text-decoration: none;
	color: #fff;
	.recipe__img{
		position:absolute;
		top: 11px;
		left: 21%;
		@include sized-sprite($icons, item);
	}
	.book__img{
		position:absolute;
		top: 14px;
		left: 25%;
		@include sized-sprite($icons, item2);
	}
	.zoom__img{
		position:absolute;
		top: 7px;
		left: 13%;
		@include sized-sprite($icons, item3);
	}
	.tioma__img{
		position:absolute;
		top: 9px;
		left: 14%;
		@include sized-sprite($icons, item4);
	}
	.photo__img{
		position:absolute;
		top: 5px;
		left: 27%;
		@include sized-sprite($icons, item5);
	}
	&:hover{
		color: #ffc979;
		.recipe__img{
		position:absolute;
		top: 11px;
		left: 21%;
		@include sized-sprite($icons, item1-hover);
	}
	.book__img{
		position:absolute;
		top: 14px;
		left: 25%;
		@include sized-sprite($icons, item2-hover);
	}
	.zoom__img{
		position:absolute;
		top: 7px;
		left: 13%;
		@include sized-sprite($icons, item3-hover);
	}
	.tioma__img{
		position:absolute;
		top: 9px;
		left: 14%;
		@include sized-sprite($icons, item4-hover);
	}
	.photo__img{
		position:absolute;
		top: 5px;
		left: 27%;
		@include sized-sprite($icons, item5-hover);
	}
	}
}
.photo_act{
	width: 94px;
	background-color: #fff;
	color: #3f699f;
	&:before{
		content: "";
		display: block;
		position: absolute;
		left: -7px;
		bottom: 0px;
		@include sized-sprite($icons, corner-left);	
	}
	&:after{
		content: "";
		display: block;
		position: absolute;
		right: -8px;
		bottom: 0px;
		@include sized-sprite($icons, corner-right);
	}
	.photo__img{
		position:absolute;
		top: 5px;
		left: 27%;
		@include sized-sprite($icons, item5-act);
	}
}	
.sub-menu{
	position: absolute;
	top: 100px;
	left: 0px;
	width: 1000px;
	height: 72px;
	padding:30px 200px 0px;
	border-bottom: 2px solid #2d6aa7;
	background: url(../img/submenu-bg.png) repeat-x;
	text-align: center;	
}
.sub-menu__item{
	display: inline-block;
	position: relative;
	a{
		padding: 0px 65px;
		font: 21px 'OpenSans-b';
		color: #3f699f;
	}
}
.sub-menu_act{
	i{
		display: block;
		position: absolute;
		bottom: -45px;
		left: 50%;
		@include sized-sprite($icons, sub-menu-act);
	}
}
.user-profile{
	position: absolute;
	top: 0px;
	right: 445px;
	width: 100px;
	padding-top: 11px;
	text-align: center;
	p{
		margin-top: -4px;
		font: 12px 'OpenSans-r';
		color: #fff;
		span{
			display: block;
		}
	}
	&:hover{
		p{
			color: #ffc979;
		}
	}
}
.get-recipe{
	position: absolute;
	top: 0px;
	right: 207px;
	padding-top: 5px;
	@include sized-sprite($icons, get-recipe);
	@include box-shadow(0px -40px 15px #000);
	text-align: center;
	font: bold 16px 'Segoe Print';
	line-height: 16px;
	color: red;
	&:hover{
		@include sized-sprite($icons, get-recipe-hover);
	}
}			

// css-styles for footer
.footer{
	overflow:hidden;
	height: 130px;
	width: 1346px;
	margin: -167px auto 0px;
	padding: 19px 23px 0px 28px;
	border-top: 1px solid #d0d0d0;
	@include box-shadow(0px -1px 1px #f0f0f0);
	background-color: #fff;
}
.footer__links{
	float: Left;
	.danone{
		float: left;
		margin: 4px 9px 0px 0px;
		@include sized-sprite($icons, danone);
	}
	p{
		margin: 8px 0px 0px 87px;;
		font: bold 13px 'OpenSans-l';
		color: #b6a38c;
	}
	.facebook{
		float: left;
		margin: 3px 0px 0px 20px;
		@include sized-sprite($icons, facebook);
	}
	.vk{
		float: left;
		margin: 3px 0px 0px 6px;
		@include sized-sprite($icons, vk);
	}
}
.footer__factory{
	float:left;
	width: 163px;
	height: 54px;
	margin-left: 325px;
	padding: 20px 7px 0px 0px;
	border-right: 1px solid #dcd6cf;
	color: #747474;
	&:hover{
		text-decoration: underline;
	}
	i{
		float: left;
		@include sized-sprite($icons, factory);
	}
	p{
		padding-left: 70px;
		font: bold 14px 'OpenSans-l';
	}
}
.footer__phone{
	float: left;
	width: 307px;
	margin: 26px 0px 0px 31px;
	i{
		float: left;
		margin-right: 5px;
		@include sized-sprite($icons, phone);
	}

}
.work-time{
	font: 14px 'OpenSans-l';
	color: #747474;
	span{
		font:bold 14px 'OpenSans-b';
	}
}
.number{
	font:bold 14px 'OpenSans-b';
	color: #4173ac; 
}
.switch{
	float: right;
	margin-top: 38px;
	font: 13px 'OpenSans-l';
}
.switcher{
	float:left;
	width: 38px;
	height: 22px;
	padding-top: 5px;
	text-align: center;
	@include border-radius(19px);
	color: #b2a087;
}
.switcher_act{
	color: #fff;
	background-color: #bca992;
}
// button 
.button{
	display:block;
	width: 186px;
	height: 42px;
	margin-top: -3px;
	padding-top: 8px;
	text-align: center;
	@include border-radius(10px);
	@include background-image(linear-gradient(top, #66a4cc, #2c5081));
	@include box-shadow(inset 0 0px 10px #8cbde3, 0 3px 3px #ccc);
	font: 21px 'OpenSans-l';
	color: white;
	&:hover{
		@include background-image(linear-gradient(top, #3f609f, #112c62));
	} 
}
.member-voice{
	float:left;
	width: 324px;
	margin: 22px 5px;
	.small-photo{
		position: relative;
		overflow: hidden;
		width: 302px;
		height: 241px;
		padding:10px;
		background-color: #fff;
		@include border-radius(10px);
		@include box-shadow(0 0px 12px #ccc);
		i{
			display: block;
			position: absolute;
			bottom: 3px;
			left: 128px;
			@include sized-sprite($icons, small-logo);
		}
		.top{
			display: block;
			position: absolute;
			top:2px;
			left: 15px;
			@include sized-sprite($icons, small-b-top);	
		}
		.right{
			display: block;
			position: absolute;
			top:8px;
			right: 7px;
			@include sized-sprite($icons, small-b-right);	
		}
		.bot{
			display: block;
			position: absolute;
			bottom:25px;
			left: 0px;
			@include sized-sprite($icons, small-b-bot);	
		}
		.left{
			display: block;
			position: absolute;
			top:0px;
			left: 7px;
			@include sized-sprite($icons, small-b-left);	
		}
	}
	.member__name{
		text-align: center;
		font: 18px 'OpenSans-r';
		color: #7a7a7a;
	}
	.member__comment{
		margin-bottom: 11px;
		text-align: center;
		font: 18px 'OpenSans-r';
	}
	.member__likes{
		position: relative;
		overflow: hidden;
		width: 219px;
		height: 44px;
		margin-left: 50px;
		@include border-radius(19px);
		@include box-shadow(0px 4px 5px #ccc);
		@include background-image(linear-gradient(top, #66a4cc, #2c5081));
		@include box-shadow(inset 0 0px 10px #8cbde3, 0 3px 3px #ccc);
		.likes-button{
			float: left;
			display: block;
			width: 127px;
			height: 37px;
			padding-top: 7px;
			text-align: center;
			font: 18px 'OpenSans-l';
			color: white;
			&:hover{
				@include border-radius(19px 0px 0px 19px);
				@include background-image(linear-gradient(top, #3f609f, #112c62));
				@include box-shadow(inset 0 0px 10px #8cbde3, 0 3px 3px #ccc);
			} 
		}
		.likes-number{
			float: right;
			width: 91px;
			height: 34px;
			padding-top: 10px;
			@include background-image(linear-gradient(top, #e1e1e1, #fefefe));
			font: 18px 'OpenSans-b';
			color: #6d6560;
			i{
				position: absolute;
				right: 70px;
				top: 16px;
				margin-left: 11px;
				@include sized-sprite($icons, star);
			}
			.likes-number__item{
				text-align: center;
			}
		}
	}
}
.big-photo{
		position: relative;
		// overflow: hidden;
		width: 636px;
		height: 438px;
		padding: 10px;
		background: url(../img/fon.png);
		@include border-radius(10px);
		@include box-shadow(0 0px 12px #ccc);
		i{
			display: block;
			position: absolute;
			bottom: 10px;
			left: 40%;
			@include sized-sprite($icons, logo);
		}
		.top{
			display: block;
			position: absolute;
			top:2px;
			left: 26px;
			@include sized-sprite($icons, big-b-top);	
		}
		.right{
			display: block;
			position: absolute;
			top: 20px;
			right: 7px;
			@include sized-sprite($icons, big-b-right);	
		}
		.bot{
			display: block;
			position: absolute;
			bottom: 14px;
			left: 5px;
			@include sized-sprite($icons, big-b-bot);	
		}
		.left{
			display: block;
			position: absolute;
			top: 8px;
			left: 8px;
			@include sized-sprite($icons, big-b-left);	
		}
